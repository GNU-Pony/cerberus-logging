RUNTIME DEPENDENCIES:

	coreutils
	sh
	libc		(opt-out, for all log-login-*)
	audit		(opt-out, for log-login-audit)
	util-linux	(optional, for ~/.prelogin support)


BUILD DEPENDENCIES:

	coreutils
	make
	gcc		(opt-out, for all log-login-*)
	libc		(opt-out, for all log-login-*)
	audit		(opt-out, for log-login-audit)


INSTALL DEPENDENCIES:

	coreutils
	make

